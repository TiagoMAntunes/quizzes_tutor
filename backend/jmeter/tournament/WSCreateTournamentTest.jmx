            <BeanShellAssertion guiclass="BeanShellAssertionGui" testclass="BeanShellAssertion" testname="Tournament validation" enabled="true">
              <stringProp name="BeanShellAssertion.query">/*{
	&quot;startTime&quot; : &quot;2022-03-17 04:20&quot;,
	&quot;finishTime&quot; : &quot;2022-05-17 04:20&quot;,
	&quot;topics&quot; : [
		{
			&quot;id&quot; : 82,
			&quot;name&quot; : &quot;Main Topic&quot;,
			&quot;parentTopic&quot; : null	
		}
	],
	&quot;numberOfQuestions&quot; : 1
}*/

Object result = vars.getObject(&quot;created_tournament&quot;).get(0);
String startTime = result.get(&quot;start_time&quot;).toString();
String finishTime = result.get(&quot;finish_time&quot;).toString();
int numberOfQuestions = result.get(&quot;number_of_questions&quot;).intValue();

log.info(&quot;---------startTime: &quot; + startTime);
log.info(&quot;---------finishTime: &quot; + finishTime);
log.info(&quot;---------numberOfQuestions: &quot; + numberOfQuestions);

if (!startTime.equals(&quot;2022-03-17 04:20:00.0&quot;) || !finishTime.equals(&quot;2022-05-17 04:20:00.0&quot;) || numberOfQuestions != 1) {
	Failure = true;
	FailureMessage = &quot;Invalid arguments. Values: &quot; + startTime + &quot; &quot; + finishTime + &quot; &quot; + numberOfQuestions;	
}
</stringProp>
              <stringProp name="BeanShellAssertion.filename"></stringProp>
              <stringProp name="BeanShellAssertion.parameters"></stringProp>
